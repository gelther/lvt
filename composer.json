{
  "name": "dannyvankooten/laravel-vat",
  "description": "VAT library for Laravel",
  "keywords": ["laravel", "vat"],
  "license": "MIT",
  "authors": [
    {
      "name": "Danny van Kooten",
      "email": "hi@dvk.co"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "DvK\\Laravel\\Vat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DvK\\Tests\\Laravel\\Vat\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}